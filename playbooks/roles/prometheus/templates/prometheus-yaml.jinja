# {{ ansible_managed }}
# Prometheus configuration

global:
  scrape_interval:        15s
  evaluation_interval:    15s
  scrape_timeout:         10s

  external_labels:
      monitor: "prometheus"

scrape_configs:
  # Scrape prometheus itself
  - job_name: "prometheus"
    static_configs:
      - targets: {{ prometheus_static_targets | to_json }}
